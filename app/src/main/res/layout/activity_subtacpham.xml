<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".subtacpham">

    <TextView
        android:id="@+id/tacphamne"
        android:layout_width="286dp"
        android:layout_height="59dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="44dp"
        android:text="11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ndungne"
        android:layout_width="0dp"
        android:layout_height="424dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="12dp"
        android:text="Sóng gợn tràng giang buồn điệp điệp,
Con thuyền xuôi mái nước song song.
Thuyền về nước lại, sầu trăm ngả;
Củi một cành khô lạc mấy dòng."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tacphamne"
        app:layout_constraintTop_toBottomOf="@+id/tacphamne"
        app:layout_constraintVertical_bias="0.205"></TextView>
</androidx.constraintlayout.widget.ConstraintLayout>